{
  "expo": {
    "name": "Oncourse OPD",
    "slug": "oncourse-opd",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "oncose-opd",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-oncourse.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C91F2"
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
